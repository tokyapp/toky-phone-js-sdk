{
  "name": "toky-phone-js-sdk",
  "version": "0.5.2",
  "description": "Toky Phone JS SDK v1",
  "main": "dist/toky-sdk-alpha.js",
  "module": "dist/toky-sdk-alpha.js",
  "typings": "dist/types/index.d.ts",
  "author": "Toky team",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "webpack": "./node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "build": "npm run webpack -- --env.mode production",
    "dev": "npm run webpack-dev-server -- --env.mode development",
    "build:dev": "npm run webpack -- --env.mode development",
    "build:staging": "npm run webpack -- --env.mode staging",
    "build:types": "tsc --emitDeclarationOnly",
    "prod:debug": "npm run debug -- --env.mode production",
    "dev:debug": "npm run debug -- --env.mode development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/events": "^3.0.0",
    "@types/node": "^13.11.1",
    "@types/pusher-js": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.1.0",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "keywords": [
    "toky-sdk",
    "javascript"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "pusher-js": "^4.0.0",
    "sip.js": "^0.16.1"
  }
}
